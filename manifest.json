{
  "manifest_version": 3,
  "name": "Tab Counter",
  "version": "1.0",
  "description": "現在開いているタブの数をアイコンに表示します。",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Tab Counter",
    "default_popup": "popup.html"
  }
}
